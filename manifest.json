{
    "manifest_version": 2,
    "name": "SenecaCheating",
    "description": "Reveal answers to blank-fill question types on SenecaLearning",
    "version": "1.0.1",
    "content_scripts": [
        {
            "matches": [
                "*://app.senecalearning.com/*"
            ],
            "css": [
                "src/unhide.css"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.svg",
        "default_title": "Autofill Answer"
    }
}